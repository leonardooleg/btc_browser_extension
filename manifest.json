{
  "manifest_version": 3,
  "name": "Bitcoin Explorer",
  "version": "1.0",
  "description": "Open Bitcoin explorer page",
  "permissions": [
    "tabs",
    "storage",
    "activeTab",
    "alarms"
  ],
  "action": {
    "default_icon": {
      "16": "icon16.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png"
  }
}
